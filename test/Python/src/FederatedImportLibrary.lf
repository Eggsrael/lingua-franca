# Test the new import statement, with the import path enclosed in angle brackets, in federated LF programs
target Python

import Count from <lib/Count.lf>

reactor Printer {
  input i

  reaction(i) {=
    print(f"Count: {i.value}")
  =}
}

federated reactor {
  c = new Count()
  p = new Printer()
  c.out -> p.i
}
