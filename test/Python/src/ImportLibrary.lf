# Test the new import statement with the import path enclosed in angle brackets
target Python {
    timeout: 2 sec
}

import Count from <lib/Count.lf>

reactor Printer {
  input i

  reaction(i) {=
    print(f"Count: {i.value}")
  =}
}

main reactor {
  c = new Count()
  p = new Printer()
  c.out -> p.i
}
