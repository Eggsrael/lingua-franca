target C {
  cmake-include: ["../../c/sendreceive.cmake"],
  files: ["../../c"]
}

reactor Print {
  output out: int

  reaction(startup) -> out named sender
}

reactor Check(expected: int = 42) {  // expected parameter is for testing.
  input in: int

  reaction(in) named receiver
}

main reactor {
  s = new Print()
  p = new Check()
  s.out -> p.in
}
