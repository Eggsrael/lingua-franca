// Smoke test for checking that the `is_present_fields` is setup correctly
// for a complicated hierarchy of nested banks with actions.
target C

reactor R1 {
  logical action a1
  logical action a2
}

reactor R2 {
  r1 = new[3] R1()
  logical action a1
  logical action a2
}

reactor R3 {
  r2 = new[4] R2()
  logical action a1
  logical action a2
}

main reactor {
  r = new[2] R3()
  logical action a1
  logical action a2
}
